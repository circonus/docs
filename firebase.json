{
  "hosting": {
    "public": "public",
    "redirects": [ {
      "source": "/caql/how-tos/howto_monitor_latency_slos/",
      "destination": "https://www.circonus.com/2020/04/monitoring-latency-slos-with-histograms-and-caql/",
      "type": 301
    }, {
      "source": "/caql/how-tos/howto_debug_caql_queries/",
      "destination": "/caql/debugging/",
      "type": 301
    }, {
      "source": "/caql/how-tos/howto_create_a_caql_check/",
      "destination": "/caql/getting-started/#creating-checks",
      "type": 301
    }, {
      "source": "/circonus/on-premises/**",
      "destination": "/circonus-on-premises/",
      "type": 301
    }, {
      "source": "/circonus/**",
      "destination": "/circonus-users/",
      "type": 301
    } ],
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ]
  }
}
